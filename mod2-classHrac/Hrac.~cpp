#include "Hrac.h"
//---------------------------------------------------------------------------------------------

Hrac::Hrac(){
                cPost = ' ';
                cJmeno = ' ';
                cPrijmeni = ' ';
                iVek = 0;
                iRychlost = 0;
                iTechnika = 0;
                iSmlouva = 0;

}
//---------------------------------------------------------------------------------------------

Hrac::Hrac(char Post, String Jmeno, int Vek, int Rychlost, int Technika, int Smlouva){
                cPost = Post;
                cJmeno = Jmeno;
                iVek = Vek;
                iRychlost = Rychlost;
                iTechnika = Technika;
                iSmlouva = Smlouva;
}
//---------------------------------------------------------------------------------------------

Hrac::~Hrac(){
              
}
//---------------------------------------------------------------------------------------------

void Hrac::setPost(char Post){
                cPost = Post;
}
//---------------------------------------------------------------------------------------------

void Hrac::setJmeno(String Jmeno){
                cJmeno = Jmeno;
}
//---------------------------------------------------------------------------------------------

void Hrac::setJmeno2(String Jmeno2){
                cJmeno2 = Jmeno2;
}
//---------------------------------------------------------------------------------------------

void Hrac::setPrijmeni(String Prijmeni){
                cPrijmeni = Prijmeni;
}
//---------------------------------------------------------------------------------------------

void Hrac::setVek(int Vek){
                iVek = Vek;
}
//---------------------------------------------------------------------------------------------

void Hrac::setVek(char *Vek){
                int iPomVek = atoi(Vek);
                iVek = iPomVek;
}
//---------------------------------------------------------------------------------------------

void Hrac::setRychlost(int Rychlost){
                iRychlost = Rychlost;
}
//---------------------------------------------------------------------------------------------

void Hrac::setRychlost(char *Rychlost){
                int iPomRychlost = atoi(Rychlost);
                iRychlost = iPomRychlost;
}
//---------------------------------------------------------------------------------------------

void Hrac::setTechnika(int Technika){
                iTechnika = Technika;
}
//---------------------------------------------------------------------------------------------

void Hrac::setTechnika(char *Technika){
                int iPomTechnika = atoi(Technika);
                iTechnika = iPomTechnika;
}
//---------------------------------------------------------------------------------------------

void Hrac::setSmlouva(int Smlouva){
                iSmlouva = Smlouva;
}
//---------------------------------------------------------------------------------------------

void Hrac::setSmlouva(char *Smlouva){
                int iPomSmlouva = atoi(Smlouva);
                iSmlouva = iPomSmlouva;
}
//---------------------------------------------------------------------------------------------

char Hrac::getPost(){
                return cPost;
}
//---------------------------------------------------------------------------------------------

String Hrac::getJmeno(){
                return cJmeno;
}
//---------------------------------------------------------------------------------------------

String Hrac::getJmeno2(){
                return cJmeno2;
}
//---------------------------------------------------------------------------------------------

String Hrac::getPrijmeni(){
                return cPrijmeni;
}
//---------------------------------------------------------------------------------------------

int Hrac::getVek(){
                return iVek;
}
//---------------------------------------------------------------------------------------------

int Hrac::getRychlost(){
                return iRychlost;
}
//---------------------------------------------------------------------------------------------

int Hrac::getTechnika(){
                return iTechnika;
}
//---------------------------------------------------------------------------------------------

int Hrac::getSmlouva(){
                return iSmlouva;
}
//---------------------------------------------------------------------------------------------
